{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "index.tsx",
    "/App/**/*.ts",
    "app/**/*.ts,tsx,js,jsx",
    "conponents/**/*.ts,tsx,js,jsx",
    "App.tsx",
    "myapp.d.ts",
    "App/**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
, "app/index.tsx"  ]
}
